{
  "$schema": "https://quantnexus.io/schemas/plugin-manifest.json",
  "id": "com.quantnexus.data-source-nexus",
  "name": "data-source-nexus",
  "displayName": "Data Source Nexus",
  "version": "1.1.0",
  "description": "Market data management with multi-source support, caching, and local storage",
  "main": "./ui/dist/plugins/data-source-nexus/ui/src/index.js",
  "backend": "./backend/dist/plugins/data-source-nexus/backend/src/index.js",
  "type": "nexus",
  "lifecycle": {
    "onInstall": "./scripts/install.js",
    "onUpgrade": "./scripts/upgrade.js",
    "onUninstall": "./scripts/uninstall.js"
  },
  "category": "data",
  "protocols": ["data-nexus"],
  "contributes": {
    "dataFeeds": [{
      "id": "nexus.local-data",
      "name": "Local Data Store",
      "adapter": "sqlite",
      "description": "SQLite/ClickHouse cached market data"
    }, {
      "id": "nexus.clickhouse",
      "name": "ClickHouse Provider",
      "adapter": "clickhouse",
      "description": "Remote ClickHouse data source"
    }],
    "viewsContainers": {
      "sidebar": [{
        "id": "dataExplorer",
        "title": "DATA",
        "icon": "resources/data-icon.svg"
      }]
    },
    "views": {
      "dataExplorer": [{
        "id": "data.tree",
        "name": "Data Sources"
      }],
      "workbench": [{
        "id": "data.config",
        "name": "Data Configuration"
      }]
    },
    "commands": [
      { "id": "data.refresh", "title": "Refresh Data" },
      { "id": "data.clearCache", "title": "Clear Cache" },
      { "id": "data.import", "title": "Import Data" },
      { "id": "data.export", "title": "Export Data" }
    ]
  },
  "permissions": ["network", "database", "filesystem"],
  "isolation": "trusted",
  "activationEvents": ["onStartup"]
}