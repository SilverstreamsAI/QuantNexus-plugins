{
  "$schema": "https://quantnexus.io/schemas/plugin-manifest.json",
  "id": "com.quantnexus.strategy-builder-nexus",
  "name": "strategy-builder-nexus",
  "displayName": "Nona Strategy Builder",
  "version": "1.0.0",
  "description": "Strategy creation, market regime classification, and AI-powered code generation",
  "main": "./dist/index.js",
  "type": "nexus",
  "lifecycle": {
    "onInstall": "./scripts/install.js",
    "onUpgrade": "./scripts/upgrade.js",
    "onUninstall": "./scripts/uninstall.js"
  },
  "category": "strategy",
  "protocols": ["strategy-nexus"],
  "entitlements": {
    "tierMapping": {
      "free": 0,
      "pro": 1,
      "gold": 1
    },
    "services": [
      {
        "id": "regime_mode",
        "name": "Regime Detector",
        "description": "Market environment and regime classification",
        "tier": "free",
        "defaultEnabled": true,
        "category": "REGIME MODE"
      },
      {
        "id": "indicator_entry_generator",
        "name": "INDICATOR ENTRY GENERATOR",
        "description": "Generate entry signals from technical indicators",
        "tier": "free",
        "defaultEnabled": true,
        "category": "REGIME MODE"
      },
      {
        "id": "kronos_predictor",
        "name": "KRONOS PREDICTOR",
        "description": "Time-series prediction engine",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "KRONOS MODE"
      },
      {
        "id": "indicator_entry",
        "name": "INDICATOR ENTRY",
        "description": "Indicator-based entry signals",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "KRONOS MODE"
      },
      {
        "id": "ai_entry",
        "name": "AI ENTRY",
        "description": "AI-powered entry signal generation",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "KRONOS MODE"
      },
      {
        "id": "market_observer",
        "name": "MARKET OBSERVER",
        "description": "Real-time market observation",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "TRADER MODE"
      },
      {
        "id": "ai_entry_generator",
        "name": "AI ENTRY GENERATOR",
        "description": "AI-powered entry generation for trading",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "TRADER MODE"
      },
      {
        "id": "indicator_exit_generator",
        "name": "INDICATOR EXIT GENERATOR",
        "description": "Risk management and exit strategy generation",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "RISK MANAGER"
      },
      {
        "id": "ai_libero",
        "name": "AI LIBERO",
        "description": "Centralized AI control and autonomous agents",
        "tier": "pro",
        "defaultEnabled": true,
        "category": "AI COMMAND CENTER"
      }
    ]
  },
  "hub": {
    "contributes": ["nona_algorithm"],
    "consumes": ["nona_algorithm"]
  },
  "contributes": {
    "i18n": {
      "path": "./locales",
      "namespaces": ["strategy-builder"]
    },
    "strategies": [],
    "configuration": {
      "title": "Strategy Builder Settings",
      "properties": {
        "strategy.dataSource": {
          "type": "string",
          "enum": ["local", "remote", "hybrid"],
          "enumDescriptions": [
            "Store data locally only",
            "Store data on remote server",
            "Use both local and remote storage"
          ],
          "default": "local",
          "description": "Data storage mode for strategies",
          "category": "Data",
          "order": 1
        },
        "strategy.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for strategy service authentication",
          "secret": true,
          "category": "Credentials",
          "order": 7
        },
        "strategy.nonaToken": {
          "type": "string",
          "default": "",
          "description": "Nona platform access token",
          "secret": true,
          "category": "Credentials",
          "order": 8
        },
        "llm.selectedProvider": {
          "type": "string",
          "enum": ["NONA", "CLAUDE", "OPENAI", "GEMINI", "DEEPSEEK", "GROK", "QWEN"],
          "enumDescriptions": [
            "Nona Fast (Default)",
            "Anthropic Claude",
            "OpenAI GPT",
            "Google Gemini",
            "DeepSeek",
            "xAI Grok",
            "Alibaba Qwen"
          ],
          "default": "NONA",
          "description": "Active LLM Provider for code generation",
          "category": "LLM Settings",
          "order": 9
        },
        "llm.selectedModel": {
          "type": "string",
          "default": "nona-fast",
          "description": "Active LLM Model ID",
          "category": "LLM Settings",
          "order": 9
        },
        "llm.claude.apiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic Claude API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 10
        },
        "llm.openai.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 11
        },
        "llm.gemini.apiKey": {
          "type": "string",
          "default": "",
          "description": "Google Gemini API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 12
        },
        "llm.deepseek.apiKey": {
          "type": "string",
          "default": "",
          "description": "DeepSeek API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 13
        },
        "llm.grok.apiKey": {
          "type": "string",
          "default": "",
          "description": "xAI Grok API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 14
        },
        "llm.qwen.apiKey": {
          "type": "string",
          "default": "",
          "description": "Alibaba Qwen API Key",
          "secret": true,
          "category": "LLM Providers",
          "order": 15
        }
      }
    },
    "viewsContainers": {
      "sidebar": [{
        "id": "strategyExplorer",
        "title": "STRATEGY",
        "icon": "resources/strategy-icon.svg"
      }]
    },
    "views": {
      "strategyExplorer": [{
        "id": "strategy.tree",
        "name": "Strategy Tree"
      }],
      "workbench": [{
        "id": "strategy.hub",
        "name": "Strategy Hub"
      }, {
        "id": "strategy.providerPortal",
        "name": "Provider Portal"
      }, {
        "id": "strategy.groupList",
        "name": "Strategy Groups"
      }],
      "settings": [{
        "id": "strategy.settings",
        "name": "Settings",
        "component": "PluginSettingsPage",
        "isMainPage": true
      }, {
        "id": "strategy.llmSettings",
        "name": "LLM Providers",
        "component": "LLMSettingsPanel"
      }]
    },
    "editors": [
      {
        "viewType": "strategy.regimeEditor",
        "displayName": "Regime Detector",
        "serviceIds": ["regime_mode"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.entrySignal",
        "displayName": "Entry Signal Generator",
        "serviceIds": ["indicator_entry_generator"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.kronosPredictor",
        "displayName": "Kronos AI Predictor",
        "serviceIds": ["kronos_predictor"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.kronosIndicatorEntry",
        "displayName": "Kronos Indicator Entry",
        "serviceIds": ["indicator_entry"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.kronosAIEntry",
        "displayName": "Kronos AI Entry",
        "serviceIds": ["ai_entry"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.marketObserver",
        "displayName": "Market Observer",
        "serviceIds": ["market_observer"],
        "selector": [{ "resourceScheme": "strategy" }]
      },
      {
        "viewType": "strategy.traderAIEntry",
        "displayName": "AI Entry Generator",
        "serviceIds": ["ai_entry_generator"],
        "selector": [{ "resourceScheme": "strategy" }]
      }
    ],
    "commands": [
      { "id": "strategy.openHub", "title": "Open Strategy Hub" },
      { "id": "strategy.generate", "title": "Generate Strategy Code" },
      { "id": "strategy.validate", "title": "Validate Strategy" },
      { "id": "strategy.register", "title": "Register Strategy to Core" }
    ]
  },
  "permissions": ["network", "database"],
  "isolation": "trusted",
  "activationEvents": ["onView:strategy.tree", "onCommand:strategy.openHub"]
}
